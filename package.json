{
  "name": "tutor-app",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "commit": "git add . && git commit -m 'fix' && git push",
    "build": "vue-cli-service build && cp -r dist/* ../../Firestore/public/",
    "host": "cd ../../Firestore && firebase deploy --only hosting && cd ../GIT/in-ynierka2",
    "full": "npm run build && npm run host",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@coreui/coreui": "^5.0.0",
    "@coreui/vue": "^5.0.0",
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.3",
    "@paypal/paypal-js": "^8.0.4",
    "@types/winston": "^2.4.4",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "base64url": "^3.0.1",
    "bootstrap": "^5.3.3",
    "core-js": "^3.8.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase": "^10.11.0",
    "firebase-admin": "^12.0.0",
    "firebaseui": "^6.1.0",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "register-service-worker": "^1.7.2",
    "usemodal-vue3": "^0.3.7",
    "vdialog": "^1.6.0",
    "vue": "^3.4.27",
    "vue-cal": "^4.8.1",
    "vue-gtag-next": "^1.14.0",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.24.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-jest": "^27.0.6",
    "babelify": "^10.0.0",
    "cypress": "^13.9.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "first-input-delay": "^0.1.3",
    "jest": "^27.0.5",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.jsx": "vue-cli-service lint"
  }
}
